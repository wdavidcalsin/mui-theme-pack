{
  "name": "mui-theme-pack",
  "private": false,
  "version": "0.9.0",
  "description": "It is a package containing a variety of themes designed to be used in your Material UI projects. In addition, you can easily switch the mode between light and dark using the hook provided. Each theme has been carefully created to bring a visually appealing and consistent look to your project. Simplify your project design with this set of preset themes and give your application a professional touch!",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wdavidcalsin/mui-theme-pack"
  },
  "homepage": "https://github.com/wdavidcalsin/mui-theme-pack",
  "scripts": {
    "dev": "tsup --watch src/index.ts",
    "build": "tsup src/index.ts",
    "lint": "eslint \"*/**/*.{js,ts,tsx}\" --fix",
    "prettier": "prettier --ignore-path .gitignore \"**/*.{js,json,ts,tsx,scss,css,md}\"",
    "format": "pnpm run prettier --write",
    "prepare": "husky install"
  },
  "peerDependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@mui/material": "^5.11.9",
    "@types/react": "^17.0.0 || ^18.0.0",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/material": "^5.11.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.52.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitejs/plugin-react-swc": "^3.1.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.0",
    "pathe": "^1.1.0",
    "prettier": "^2.8.4",
    "prettier-eslint": "^15.0.1",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "vite": "^4.1.0",
    "vite-plugin-dts": "2.0.0-beta.0"
  },
  "keywords": [
    "mui-theme-pack",
    "mui",
    "material ui",
    "@material-ui",
    "react",
    "react-component",
    "material design"
  ],
  "author": "Willian David",
  "license": "MIT"
}
